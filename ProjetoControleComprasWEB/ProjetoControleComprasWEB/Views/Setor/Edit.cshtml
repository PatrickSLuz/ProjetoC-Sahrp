@model Domain.Setor

@{
    ViewData["Title"] = "Editar Setor";
}

<h2>Editar Setor</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="SetorId" />
            <div class="form-group">
                <label asp-for="NomeSetor" class="control-label"></label>
                <input asp-for="NomeSetor" class="form-control" />
                <span asp-validation-for="NomeSetor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
